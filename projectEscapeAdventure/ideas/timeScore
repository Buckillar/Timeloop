<html>
<head>
</head>    
<body>

    <div id="timer">02:02</div>
    <button onclick="dance()">Dance</button>
    <script>
        // Get the content of the timer div
        var timerDiv = document.getElementById("timer");
        var timerValue = timerDiv.textContent;
    
        // Split the timer value into minutes and seconds
        var timeArray = timerValue.split(":");
        var minutes = parseInt(timeArray[0], 10);
        var seconds = parseInt(timeArray[1], 10);
    
        // Calculate the total seconds
        var totalSeconds = minutes * 60 + seconds;
    
        console.log("Total seconds: " + totalSeconds);
    </script>
</body>
<html>
